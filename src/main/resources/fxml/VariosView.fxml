<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="view" alignment="CENTER" prefHeight="200.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="holaLabel" style="-fx-font-size: 30pt;" text="¡Hola Mundo!" />
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Button fx:id="animacion1Button" mnemonicParsing="false" onAction="#onAnimacion1Action" text="Animación 1" />
            <Button fx:id="animacion2Button" layoutX="120.0" layoutY="10.0" mnemonicParsing="false" onAction="#onAnimacion2Action" text="Animación 2" />
            <Button fx:id="animacion3Button" layoutX="170.0" layoutY="10.0" mnemonicParsing="false" onAction="#onAnimacion3Action" text="Animación 3" />
         </children>
      </HBox>
   </children>
</VBox>
