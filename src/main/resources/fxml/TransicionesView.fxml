			<tabs>
				<Tab fx:id="fadeTab" text="Fade" />
				<Tab fx:id="fillTab" text="Fill" />
				<Tab fx:id="pathTab" text="Path" />
				<Tab fx:id="pauseTab" text="Pause" />
